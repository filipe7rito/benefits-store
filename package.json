    "@types/testing-library__jest-dom": "^5.14.5",
    "@vitejs/plugin-react": "^3.0.0",
    "@vitest/coverage-istanbul": "^0.28.4",
    "@vitest/ui": "^0.28.4",
    "jsdom": "^21.1.0",
    "msw": "^1.0.0",
    "prettier": "^2.8.2",
    "sass": "^1.57.1",
    "start-server-and-test": "^1.15.3",
    "typescript": "^4.9.3",
    "vite": "^4.1.1",
    "vitest": "^0.28.4"
  }
